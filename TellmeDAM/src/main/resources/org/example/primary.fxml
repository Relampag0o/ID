<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label style="-fx-font: bold;" text="LOG IN">
         <font>
            <Font name="Khmer OS System" size="21.0" />
         </font>
      </Label>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
      <Label text="Email" />
      <TextField fx:id="emailField" />
      <Label text="Password" />
      <TextField fx:id="passwordField" />
      <Label alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#switchToSecondary" prefHeight="16.0" prefWidth="230.0" text="New user? Register now!" />
      <Button mnemonicParsing="false" onAction="#login" prefHeight="40.0" prefWidth="96.0" text="Log in" />
   </children>
</VBox>
